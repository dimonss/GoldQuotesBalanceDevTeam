  "description": "A project that should store golden quotes from Dev Team",
  "main": "src/index.js",
  "type": "module",
  "compilerOptions": {
    "esModuleInterop": true
  },
  "scripts": {
    "start:dev": "nodemon src/index.js",
    "start:prod": "pm2 start src/index.js --name GoldenQuotes",

